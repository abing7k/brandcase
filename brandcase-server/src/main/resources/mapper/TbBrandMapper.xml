<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.server.mapper.TbBrandMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.server.pojo.TbBrand">
        <id column="id" property="id" />
        <result column="brand_name" property="brandName" />
        <result column="company_name" property="companyName" />
        <result column="ordered" property="ordered" />
        <result column="description" property="description" />
        <result column="status" property="status" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, brand_name, company_name, ordered, description, status
    </sql>

</mapper>
